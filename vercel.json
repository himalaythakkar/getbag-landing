{
    "version": 2,
    "installCommand": "npm install --legacy-peer-deps",
    "outputDirectory": "build",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}